let search = (arr: int[], n: int, start: int, end: int): int => {
  if start >= end {
    return -1;
  }
  let mid = (start + end)/2;
  if arr[mid] == n {
    return mid;
  }
  if arr[mid] > n {
    search(arr, n, start, mid - 1)
  } else {
    search(arr, n, mid + 1, end)
  }
}

let main = () => {
  let res = search([1, 4, 5, 10], 10, 0, 3);
  printInt(res);
}

export main;